<%= render partial: 'layouts/header' %>

<h3>Create a new post </h3>
<form class="new-post" action="<%= posts_path %>" method="post">
  <input type="text" name="post[song_title]" placeholder= "song title">
  <input type="text" name="post[artist_name]" placeholder="artist name">
  <input type="text" name="post[vibe]" placeholder="vibe">
</form>
